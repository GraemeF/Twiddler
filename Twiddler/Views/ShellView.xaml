<Window x:Class="Twiddler.Views.ShellView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:cal="clr-namespace:Caliburn.PresentationFramework.Views;assembly=Caliburn.PresentationFramework"
        Title="Twiddler"
        MinHeight="300"
        MinWidth="300">
    <DockPanel VerticalAlignment="Stretch">
        <ContentControl cal:View.Model="{Binding Status}"
                        DockPanel.Dock="Top" />
        <ContentControl cal:View.Model="{Binding Timeline}" />
    </DockPanel>
</Window>